<div class="main" style="padding:0 50px;">
    <p class="title">Your personal area:</p>
    <div class="info-container">
        <div class="profile-pic">
            <img src="http://vignette2.wikia.nocookie.net/filthy-frank/images/c/ce/Question-mark-face.jpg/revision/latest?cb=20160909100759"/>
        </div>
        <div class="personal-info">
            {% for key, value in map %}
            <div class="info"><p class="data">{{ key }}</p><p class={{ value }}></div>
        </div>
    </div>
</div>