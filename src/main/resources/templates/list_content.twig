<div class="listing">
    <ol>
        {% for key, value in data %}
            <div class="row">
                <li id="hidden-buttons"><label><a href="{{ key }}">{{ value }}</a></label>
                    <button value="edit" onclick="window.open('{{ key }}/edit', '_parent')">Edit</button>
                    <button value="remove" onclick="window.open('{{ key }}/remove', '_parent')">Remove</button>
                </li>
            </div>
        {% endfor %}
    </ol>
</div>