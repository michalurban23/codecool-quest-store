<div class="personal-info">
    <form method="get">
        {% for key, value in data %}
            <div class="row">
                <label>{{ key }}</label>
                <p>
                    {% if (value) %}
                    {{ value }}
                    {% else %}
                    -
                    {% endif %}
                </p>
            </div>
        {% endfor %}
    </form>
</div>

